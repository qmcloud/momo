#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_atob@2.1.2@atob/bin/atob.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_atob@2.1.2@atob/bin/atob.js" "$@"
  ret=$?
fi
exit $ret
