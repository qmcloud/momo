older=\"请选择\">\r\n            <el-option key=\"mysql\" label=\"mysql(目前只支持mysql)\" value=\"mysql\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"host\">\r\n          <el-input v-model=\"form.host\" placeholder=\"请输入数据库链接\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"port\">\r\n          <el-input v-model=\"form.port\" placeholder=\"请输入数据库端口\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"userName\">\r\n          <el-input v-model=\"form.userName\" placeholder=\"请输入数据库用户名\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"password\">\r\n          <el-input\r\n            v-model=\"form.password\"\r\n            placeholder=\"请输入数据库密码（没有则为空）\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"dbName\">\r\n          <el-input v-model=\"form.dbName\" placeholder=\"请输入数据库名称\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <div style=\"text-align: right\">\r\n            <el-button type=\"primary\" @click=\"onSubmit\">立即初始化</el-button>\r\n          </div>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { initDB } from \"@/api/initdb\";\r\nexport default {\r\n  name: \"Init\",\r\n  data() {\r\n    return {\r\n      form: {\r\n        sqlType: \"mysql\",\r\n        host: \"127.0.0.1\",\r\n        port: \"3306\",\r\n        userName: \"root\",\r\n        password: \"\",\r\n        dbName: \"gva\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async onSubmit() {\r\n      const loading = this.$loading({\r\n        lock: true,\r\n        text: \"正在初始化数据库，请稍候\",\r\n        spinner: \"el-icon-loading\",\r\n        background: \"rgba(0, 0, 0, 0.7)\",\r\n      });\r\n      try {\r\n        const res = await initDB(this.form);\r\n        if (res.code == 0) {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: res.msg,\r\n          });\r\n          this.$router.push({name:\"login\"})\r\n        }\r\n          loading.close();\r\n      } catch (err) {\r\n          loading.close();\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\">\r\n.init {\r\n  height: 100vh;\r\n  flex-direction: column;\r\n  display: flex;\r\n  align-items: center;\r\n  background: #fff;\r\n}\r\n.in-one {\r\n  font-size: 26px;\r\n  line-height: 98px;\r\n}\r\n.in-two {\r\n  font-size: 22px;\r\n}\r\n.form-card {\r\n  margin-top: 60px;\r\n  box-shadow: 0px 0px 5px 0px rgba(5, 12, 66, 0.15);\r\n  width: 60vw;\r\n  height: auto;\r\n  background: #fff;\r\n  padding: 20px;\r\n  border-radius: 6px;\r\n}\r\n</style>\r\n"]}]}