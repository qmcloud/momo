#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_is-ci@1.2.1@is-ci/bin.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_is-ci@1.2.1@is-ci/bin.js" "$@"
  ret=$?
fi
exit $ret
